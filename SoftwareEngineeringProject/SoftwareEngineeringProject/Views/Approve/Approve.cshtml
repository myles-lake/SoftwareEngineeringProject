@model IEnumerable<SoftwareEngineeringProject.Data.KeyRequestLines>
<h2>Approve Keys</h2>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Banner ID</th>
            <th>Email</th>
            <th>Room Number</th>
            <th>Access type</th>
            <th>Reason</th>
            <th>Approve</th>
            <th>Disapprove</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.KeyRequest.Requestor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.KeyRequest.ApplicationUser.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RoomID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Room.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ReasonForAccess)
                </td>
                <td>
                    <a asp-action="ApproveKey" asp-route-id="@item.Id">Approve</a>
                </td>
                <td>
                    <a asp-action="DissapproveKey" asp-route-id="@item.Id">Disapprove</a>
                </td>
            </tr>
        }
    </tbody>
</table>